lImages = 50; // Cambia según el total de imágenes que tengas\r\n      return Array.from({ length: totalImages }, (_, i) =>\r\n        this.generateImageUrl(i + 1)\r\n      );\r\n    },\r\n    generateImageUrl(index) {\r\n      // Función que genera la URL de las imágenes\r\n      return `/real-academy-fc/fotos-real/foto-real-facup-${index}.jpeg`;\r\n    },\r\n    openModal(imageUrl) {\r\n      this.modalImage = imageUrl;\r\n      this.modalVisible = true;\r\n      document.body.style.overflow = \"hidden\"; // Desactiva el scroll del sitio\r\n    },\r\n    closeModal() {\r\n      this.modalVisible = false;\r\n      document.body.style.overflow = \"\"; // Reactiva el scroll del sitio\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\nbody {\r\n  margin: 0;\r\n}\r\n\r\n.container {\r\n  position: relative;\r\n}\r\n\r\n@keyframes scroll {\r\n  0% {\r\n    transform: translateX(0);\r\n  }\r\n  100% {\r\n    transform: translateX(calc(-400px * 60)); /* Ajusta para la cantidad de imágenes */\r\n  }\r\n}\r\n\r\n.slider {\r\n  background: black;\r\n  height: 500px;\r\n  margin: auto;\r\n  overflow: hidden;\r\n  position: relative;\r\n  width: 100vw;\r\n}\r\n\r\n.slide-track {\r\n  animation: scroll 120s linear infinite; /* Velocidad más rápida */\r\n  display: flex;\r\n  width: calc(350px * 100); /* Ajusta para que el carrusel cubra todas las imágenes */\r\n}\r\n\r\n.slide {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.slide img {\r\n  height: 350px;\r\n  width: 350px;\r\n  object-fit: cover;\r\n  cursor: pointer;\r\n  transition: transform 0.3s ease, box-shadow 0.3s ease;\r\n  box-shadow: 0 4px 15px rgba(0, 0, 0, 0.2);\r\n  margin: 20px;\r\n  border-radius: 50%;\r\n  position: relative;\r\n  top: 100px;\r\n}\r\n\r\n.slide img:hover {\r\n  transform: scale(1.1);\r\n  box-shadow: 0 8px 20px rgba(0, 0, 0, 0.3);\r\n}\r\n\r\n/* Estilos del modal */\r\n.modal-overlay {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100vw;\r\n  height: 100vh;\r\n  background: rgba(0, 0, 0, 0.8);\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  z-index: 1000;\r\n  opacity: 0;\r\n  animation: fadeIn 0.5s forwards;\r\n}\r\n\r\n@keyframes fadeIn {\r\n  to {\r\n    opacity: 1;\r\n  }\r\n}\r\n\r\n.modal-content {\r\n  position: relative;\r\n}\r\n\r\n.close {\r\n  position: absolute;\r\n  top: 10px;\r\n  right: 20px;\r\n  font-size: 24px;\r\n  color: white;\r\n  cursor: pointer;\r\n  transition: transform 0.3s ease;\r\n}\r\n\r\n.close:hover {\r\n  transform: scale(1.2);\r\n}\r\n\r\n.modal-content img {\r\n  max-width: 90vw;\r\n  max-height: 90vh;\r\n  object-fit: contain;\r\n  border-radius: 20px;\r\n}\r\n</style>\r\n"],"mappings":";;EACOA,KAAK,EAAC,WAAW;EAACC,EAAE,EAAC;;;EACnBD,KAAK,EAAC;AAAQ;;EACZA,KAAK,EAAC;AAAa;mBAH9B;mBAAA;;uBACEE,mBAAA,CAoBM,OApBNC,UAoBM,GAnBJC,mBAAA,CAUM,OAVNC,UAUM,GATJD,mBAAA,CAQM,OARNE,UAQM,I,kBAPJJ,mBAAA,CAMMK,SAAA,QAVdC,WAAA,CAMmCC,QAAA,CAAAC,iBAAiB,EANpD,CAMkBC,KAAK,EAAEC,KAAK;yBAFtBV,mBAAA,CAMM;MALJF,KAAK,EAAC,OAAO;MAEZa,GAAG,aAAaD;QAEjBR,mBAAA,CAAoE;MAA9DU,GAAG,EAAEH,KAAK;MAAGI,OAAK,EAAAC,MAAA,IAAEP,QAAA,CAAAQ,SAAS,CAACN,KAAK;MAAGO,OAAO,EAAC,MAAM;MAACC,GAAG,EAAC;4BATzEC,UAAA,E;sCAcIC,mBAAA,uDAA0D,EAC/CC,KAAA,CAAAC,YAAY,I,cAAvBrB,mBAAA,CAKM;IApBVW,GAAA;IAe6Bb,KAAK,EAAC,eAAe;IAAEe,OAAK,EAAAS,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEhB,QAAA,CAAAiB,UAAA,IAAAjB,QAAA,CAAAiB,UAAA,IAAAD,IAAA,CAAU;MAC/DrB,mBAAA,CAGM;IAHDJ,KAAK,EAAC,uBAAuB;IAAEe,OAAK,EAAAS,MAAA,QAAAA,MAAA,MAhB/CG,cAAA,CAgByC,QAAW;MAC5CvB,mBAAA,CAAsD;IAAhDJ,KAAK,EAAC,OAAO;IAAEe,OAAK,EAAAS,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEhB,QAAA,CAAAiB,UAAA,IAAAjB,QAAA,CAAAiB,UAAA,IAAAD,IAAA,CAAU;KAAE,GAAO,GAC/CrB,mBAAA,CAAgC;IAA1BU,GAAG,EAAEQ,KAAA,CAAAM,UAAU;IAAET,GAAG,EAAC;0BAlBnCU,UAAA,E,OAAAR,mBAAA,e","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}